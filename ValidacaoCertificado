    <script type="text/javascript">  
        var your_site_key = '6LcJPTUUAAAAAApYt0_OWtPKizfRIlefYYoLhl1G';
        var renderRecaptcha = function () {
            grecaptcha.render('ReCaptchContainer', {
                'sitekey': your_site_key,
                'callback': reCaptchaCallback,
                theme: 'light',
                type: 'image',
                size: 'normal'

            });

        };

        var reCaptchaCallback = function (response) {
            if (response !== '') {
                jQuery('#lblMessage').css('color', 'green').html('Successo!');

            }

        };
        jQuery('button[type="button"]').click(function (e) {
            var message = 'Click na checkbox';
            if (typeof (grecaptcha) != 'undefined') {
                var response = grecaptcha.getResponse();
                (response.length === 0) ? (message = 'Verificação mal sucedida!') : (message = 'Successo!');

            }
            jQuery('#lblMessage').html(message);
            jQuery('#lblMessage').css('color', (message.toLowerCase() == 'sucesso!') ? "green" : "red");

        });

    </script>


